{
  "name": "discord-notify-mcp",
  "version": "0.0.5",
  "type": "module",
  "bin": {
    "discord-notify-mcp": "dist/main.js"
  },
  "files": ["dist", "src", "README.md"],
  "scripts": {
    "dev": "tsdown --config tsdown.config.ts --watch",
    "build": "tsdown --config tsdown.config.ts",
